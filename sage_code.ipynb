    "def timeseries_to_supervised(data, lag):\n",
    "    df = DataFrame(data)\n",
    "    columns = [df.shift(i) for i in range(1, lag+1)]\n",
    "    columns.append(df)\n",
    "    df = concat(columns, axis=1)\n",
    "    return DataFrame(df)"
   ]
  },
  {
   "cell_type": "code",
